#include <iostream>

using namespace std;

int main() {

   int x = 3;

   switch(x) {

       case 1:

            cout << "Satu";

       case 2:

            cout << "Dua";

       case 3:

            cout << "Tiga";

       case 4:

            cout << "Empat";

       default:

            cout << "Default";

   }

   return 0;

}